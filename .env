# Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL=https://trandbackend-production.up.railway.app
VITE_WS_BASE_URL=ws://trandbackend-production.up.railway.app/

# Application Settings
VITE_APP_NAME=TradeSense AI
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_AUTH=true
VITE_ENABLE_CHALLENGES=true
VITE_ENABLE_AI_ASSISTANCE=true
VITE_ENABLE_REALTIME_PRICES=true

# Localization
VITE_DEFAULT_LANGUAGE=en
VITE_SUPPORTED_LANGUAGES=en,fr,ar

# Theme Settings
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true

# Debug Settings
VITE_ENABLE_DEBUG=false
VITE_LOG_LEVEL=info

# Security
VITE_JWT_EXPIRY=24h